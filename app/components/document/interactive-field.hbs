<div {{did-insert this.setup this}}
     class="field signer-{{@field.order}}"
     style="left: {{@field.x}}%;
            top: {{@field.y}}%;
            height: {{@field.height}}%;
            width: {{@field.width}}%"
     data-guid={{@field.guid}}>
    {{component (concat 'document/interactive-fields/' this.type) value=@field.value}}
</div>